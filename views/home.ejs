    <!-- card section -->
    <div class="container">
      <div class="mt-4">
        <div class="row">
          <div class="col-lg-4 col-md-6 col-sm-12">
            <div class="card" style="width: 18rem">
              <div id="images">
                <a href="/men">
                  <img
                    src="https://sslimages.shoppersstop.com/sys-master/images/hc8/h75/29513934143518/S23STCTNCHK40WH_WHITE.jpg_2000Wx3000H"
                    class="card-img-top man-img"
                    alt="..."
                  />
                </a>
              </div>
              <div class="card-body">
                <h5 class="card-title">Stop</h5>
                <p class="card-text">Checks Cotton Regular Fit Mens Shirts</p>
                <p>$350 <del>$800</del></p>
