            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
          </button>
        </div>
      </div>
    </div>

    <!-- crousel section3 -->
    <div class="container-fluid featuredBrands">
      <div class="container">
        <div class="mt-5">
          <h1 class="text-center text-light-emphasis">
            Featured <span class="text-dark">Brands</span>
