            <p>$750 <del>$2500</del></p>
          </div>
        </div>
      </div>
    </div>

    <!-- footer section -->
    <div class="container-fluid">
      <div class="mt-4">
        <div class="card">
          <div class="card-header">Shoppers Stop</div>
          <div class="card-body">
            <a href="/" class="card-title">www.shoppersstop.com</a>
            <p class="card-text text-center">&copy; All Rights Reserved</p>
          </div>
