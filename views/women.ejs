    <!-- favicon -->
    <link
      rel="shortcut icon"
      href="https://www.shoppersstop.com/favicon-16x16.png"
      type="image/x-icon"
    />

    <!-- CSS link -->
    <link rel="stylesheet" href="/style.css" />

    <!-- Bootstrap CDN -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
